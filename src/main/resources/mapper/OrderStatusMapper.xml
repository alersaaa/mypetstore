<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.csu.mypetstorebyssm.persistence.OrderStatusMapper">
    <insert id="insertOrderStatus" parameterType="Order">
         INSERT INTO ORDERSTATUS (ORDERID, LINENUM, TIMESTAMP, STATUS)
         VALUES (#{orderId}, #{orderId}, #{orderDate}, #{status})
    </insert>
</mapper>
